      a = int(1)
      a = int(1)
      a = int(1)
      a = int(1)
      a = int(1)
      a = int(1)
      a = int(1)
      a = int(f(int(1),g(h(int(4)))))
      a = int(f(int(1),g(h(int(4)))))

c Test "nint()"
      a = nint(1)
